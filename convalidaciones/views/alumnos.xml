<odoo>
  <data>
    <record id="convalidaciones.list_alumnos" model="ir.ui.view">
      <field name="name">Listado</field>
      <field name="model">convalidaciones.alumno</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name" />
          <field name="edad" />
        </tree>
      </field>
    </record>

    <record id="convalidaciones.action_window" model="ir.actions.act_window">
      <field name="name">Ventana de Alumnos</field>
      <field name="res_model">convalidaciones.alumno</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem
      id="convalidaciones.menu_root"
      name="[___ Convalidaciones __]"
      action="convalidaciones.action_window"/>
    <menuitem
      id="convalidaciones.alumnos"
      name="Alumnos"
      parent="convalidaciones.menu_root"/>

    <menuitem
      id="convalidaciones.alumnos_list"
      name="Listado"
      action=""
      parent="convalidaciones.menu_root"
      sequence=""/>  
  </data>
</odoo>